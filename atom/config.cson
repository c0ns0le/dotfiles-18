"*":
  "atom-ide-ui":
    "atom-ide-terminal":
      black: "#f8f8f2"
      blue: "#8be9fd"
      brightBlack: "#f8f8f2"
      brightBlue: "#8be9fd"
      brightCyan: "#8be9fd"
      brightGreen: "#50fa7b"
      brightMagenta: "#ff79c6"
      brightRed: "#ff5555"
      brightWhite: "#f8f8f2"
      brightYellow: "#f1fa8c"
      cyan: "#66d9ef"
      fontFamily: "Hack"
      green: "#50fa7b"
      magenta: "#ff79c6"
      red: "#ff5555"
      white: "#f8f8f2"
      yellow: "#e6db74"
  "atom-package-deps":
    ignored: [
      "prettier-atom"
    ]
  core:
    disabledPackages: [
      "vim-mode-plus"
      "ex-mode"
      "wrap-guide"
    ]
    telemetryConsent: "limited"
    themes: [
      "one-light-ui"
      "one-light-syntax"
    ]
    uriHandlerRegistration: "always"
  "custom-title":
    template: "<% if (projectPath) { %><%= projectName %> - <% } %><%= fileName %>"
  editor:
    fontFamily: "Hack"
  "exception-reporting":
    userId: "eec27448-57cd-4ba4-840b-c7ff0eb53af1"
  "prettier-atom":
    formatOnSaveOptions:
      enabled: true
  "vim-mode-plus":
    clearMultipleCursorsOnEscapeInsertMode: true
    keymapYToYankToLastCharacterOfLine: true
    stayOnDelete: false
    stayOnSelectTextObject: false
    stayOnTransformString: false
    stayOnVerticalMotion: false
    stayOnYank: false
    updateRegisterOnChangeOrSubstitute: true
  welcome:
    showOnStartup: false
